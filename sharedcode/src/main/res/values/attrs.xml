<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="text_font" format="enum">
        <enum name="NunitoSans_Regular" value="0" />
        <enum name="NunitoSans_Light" value="1" />
        <enum name="NunitoSans_SemiBold" value="2" />
        <enum name="NunitoSans_Bold" value="3" />
        <enum name="NunitoSans_ExtraBold" value="4" />
    </attr>

    <declare-styleable name="CustomTextView">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomTextInputLayout">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomTextInputEditText">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomCheckBox">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomRadioButton">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="BarGraph">
        <attr name="barAxisColor" format="color" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="barShowAxis" format="boolean" />
        <attr name="barShowAxisLabel" format="boolean" />
        <attr name="barShowText" format="boolean" />
        <attr name="barShowPopup" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomMaterialButton">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomMaterialChip">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomDatePicker">
        <attr name="dateOrder" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomMaterialRadioButton">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomMaterialCheckBox">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomMaterialToolbar">
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="UnderlinedTextView">
        <attr name="underlineWidth" format="dimension" />
        <attr name="underlineMarginTop" format="dimension" />
        <attr name="underlineColor" format="color" />
        <attr name="underlineText" format="string" />
        <attr name="underlineIsAll" format="boolean" />
        <attr name="underlineNoLine" format="integer" />
        <attr name="underlineTextEachLine" format="integer" />
        <attr name="text_font" />
    </declare-styleable>

    <declare-styleable name="CustomMaterialTabLayout">
        <attr name="text_font" />
    </declare-styleable>

</resources>